# Supabase project URL
SUPABASE_URL=https://YOUR-PROJECT-REF.supabase.co

# Supabase service role key (server-side only; do not expose to browser)
SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY

# Supabase Storage bucket name used for uploads
SUPABASE_STORAGE_BUCKET=grading-images

# CORS for local Vite dev
CORS_ORIGINS=http://localhost:5173,http://localhost:5174

# OpenRouter API for grading
OPENROUTER_API_KEY=YOUR_OPENROUTER_API_KEY
# Optional: override base URL (defaults to https://openrouter.ai/api/v1)
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
