{
  "name": "mark-grading-assistant",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:mac": "tauri build --target universal-apple-darwin",
    "tauri:build:win": "tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "backend:build": "pyinstaller backend_launcher.spec --clean",
    "backend:build:all": "node scripts/build-backends.js",
    "icons:generate": "tauri icon src-tauri/icons/icon.png",
    "release": "npm run build && npm run backend:build && npm run tauri:build",
    "release:all": "npm run build && npm run backend:build:all && npm run tauri:build",
    "build:desktop": "python build_desktop_app.py",
    "build:desktop:dev": "python build_desktop_app.py --dev",
    "build:desktop:clean": "python build_desktop_app.py --clean",
    "clean": "python build_desktop_app.py --clean",
    "verify:backend": "python build_desktop_app.py --verify-only",
    "package:backend": "python -c \"import backend_runner; import backend; print('Backend files ready')\"",
    "package:all": "python build_app.py"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.1.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "@tauri-apps/plugin-process": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tauri-apps/cli": "^2.0.0-rc.18",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
